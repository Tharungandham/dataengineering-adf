{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "harryadf18"
		},
		"ls_AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_AzureSqlDatabase1'"
		},
		"ls_blb_output_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blb_output'"
		},
		"ls_server_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_server'"
		},
		"ls_AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "harryserver.database.windows.net"
		},
		"ls_AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "harry_database"
		},
		"ls_AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "tharun1"
		},
		"ls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://harrystracc.dfs.core.windows.net"
		},
		"ls_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://tharunvault.vault.azure.net/"
		},
		"ls_server_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "harryserver.database.windows.net"
		},
		"ls_server_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "harry_database"
		},
		"ls_server_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "tharun1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "harryadf18_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureSqlDatabase1_password": {
						"value": "[parameters('ls_AzureSqlDatabase1_password')]"
					},
					"ls_blb_output_connectionString": {
						"value": "[parameters('ls_blb_output_connectionString')]"
					},
					"ls_server_password": {
						"value": "[parameters('ls_server_password')]"
					},
					"ls_AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('ls_AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"ls_AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('ls_AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"ls_AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('ls_AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"ls_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_properties_typeProperties_baseUrl')]"
					},
					"ls_server_properties_typeProperties_server": {
						"value": "[parameters('ls_server_properties_typeProperties_server')]"
					},
					"ls_server_properties_typeProperties_database": {
						"value": "[parameters('ls_server_properties_typeProperties_database')]"
					},
					"ls_server_properties_typeProperties_userName": {
						"value": "[parameters('ls_server_properties_typeProperties_userName')]"
					},
					"ls_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}